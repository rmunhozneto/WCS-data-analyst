SELECT `firstname`, `birthday` FROM `wizard` WHERE `birthday` BETWEEN '1970-01-01' AND '1985-12-31';
---------------------------------------------------------------------------------------------------------
harry	1980-07-31
hermione	1979-09-19
ron	1980-03-01
ginny	1981-08-11
fred	1978-04-01
george	1978-04-01
drago	1980-06-05
dudley	1980-06-23

--------------------------------------------------------------------------------------------------------
SELECT `firstname` FROM `wizard` WHERE `firstname` LIKE 'h%';
--------------------------------------------------------------------------------------------------------
harry
hermione

--------------------------------------------------------------------------------------------------------
SELECT `firstname`, `lastname` FROM `wizard` WHERE `lastname`='potter' ORDER BY `firstname` ASC;
--------------------------------------------------------------------------------------------------------
harry	potter
lily	potter

--------------------------------------------------------------------------------------------------------
SELECT `firstname`, `lastname`, `birthday` FROM `wizard` WHERE `birthday` ORDER BY `birthday` ASC LIMIT 1;
--------------------------------------------------------------------------------------------------------
albus	dumbledore	1881-07-01
